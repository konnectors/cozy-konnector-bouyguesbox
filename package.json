{
  "name": "cozy-konnector-bouyguesbox",
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-konnector-bouyguesbox.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "eslint-config-cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "cozy-run-dev",
    "standalone": "cozy-run-standalone",
    "build": "webpack",
    "format": "prettier --write '**/*.{js,json}'",
    "precommit": "yarn lint",
    "lint": "eslint .",
    "test": "konitor testit .",
    "deploy": "git-directory-deploy --directory build/ --branch build --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-bouyguesbox.git}",
    "deploy:prod": "git-directory-deploy --directory build/ --branch latest --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-bouyguesbox.git}"
  },
  "dependencies": {
    "cozy-konnector-libs": "3.7.4",
    "moment": "2.21.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "4.5.1",
    "cozy-jobs-cli": "1.0.6",
    "eslint": "4.19.0",
    "eslint-config-cozy-app": "0.5.0",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "konitor": "0.5.3",
    "prettier": "1.11.1",
    "webpack": "4.2.0",
    "webpack-cli": "2.0.13"
  }
}
